global stopR waitT clockmax nc

totalNumCarsWaiting = sum(stopR);

averageWaitTime = waitT./clockmax;
totalWaitTime = sum(averageWaitTime);
averageWaitTime = totalWaitTime / nc;

disp("Total num times a car waited: " + totalNumCarsWaiting );
disp("Cumulative average wait time: " + averageWaitTime );

